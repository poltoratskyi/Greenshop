    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "next": "^15.2.3",
    "next-auth": "^4.24.11",
    "nextjs-toploader": "^3.7.15",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-loading-skeleton": "^3.5.0",
    "react-phone-input-2": "^2.15.1",
    "react-use": "^17.6.0",
    "resend": "^4.1.2",
